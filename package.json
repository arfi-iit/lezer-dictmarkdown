  "dependencies": {
    "@lezer/common": "^1.2.3",
    "@lezer/highlight": "^1.2.1",
    "@lezer/lr": "^1.4.2"
  },
  "devDependencies": {
    "@lezer/generator": "^1.7.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "rollup": "^4.34.0",
    "mocha": "^11.1.0"
